<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
  <classPathEntry location="C:/Users/chen_maohua/.m2/repository/mysql/mysql-connector-java/5.1.9/mysql-connector-java-5.1.9.jar" />

  <context id="Tables" targetRuntime="MyBatis3">
    <jdbcConnection driverClass="com.mysql.jdbc.Driver"
        connectionURL="jdbc:mysql://192.168.134.132:3306/breeze"
        userId="breeze"
        password="breeze">
    </jdbcConnection>

    <javaTypeResolver >
      <property name="forceBigDecimals" value="false" />
    </javaTypeResolver>

    <javaModelGenerator targetPackage="org.breeze.entity" targetProject="./src">
      <property name="enableSubPackages" value="true" />
      <property name="trimStrings" value="false" />
    </javaModelGenerator>

    <sqlMapGenerator targetPackage="org.breeze.dao"  targetProject="./src">
      <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>

    <javaClientGenerator type="XMLMAPPER" targetPackage="org.breeze.dao"  targetProject="./src">
      <property name="enableSubPackages" value="true" />
    </javaClientGenerator>

	<!-- tables -->
    <table schema="breeze" tableName="users">
		<property name="ignoreQualifiersAtRuntime" value="true" />
		<property name="runtimeTableName" value="users" />
    </table>

	<table schema="breeze" tableName="roles">
		<property name="ignoreQualifiersAtRuntime" value="true" />
		<property name="runtimeTableName" value="roles" />
    </table>

	<table schema="breeze" tableName="resources">
		<property name="ignoreQualifiersAtRuntime" value="true" />
		<property name="runtimeTableName" value="resources" />
    </table>

	<table schema="breeze" tableName="users_roles">
		<property name="ignoreQualifiersAtRuntime" value="true" />
		<property name="runtimeTableName" value="users_roles" />
    </table>

	<table schema="breeze" tableName="roles_resources">
		<property name="ignoreQualifiersAtRuntime" value="true" />
		<property name="runtimeTableName" value="roles_resources" />
    </table>

  </context>
</generatorConfiguration>
